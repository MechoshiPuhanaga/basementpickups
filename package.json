  "private": true,
  "scripts": {
    "clean": "npx rimraf dist",
    "start": "yarn clean && npx concurrently \"yarn dev:client\" \"yarn dev:server\" \"yarn run:dev\"",
    "prod": "npx rimraf dist && webpack --config webpack/ssr/webpack.ssr.client.js --mode production && webpack --config webpack/ssr/webpack.ssr.server.js --mode production",
    "run:prod": "node dist/server.js",
    "dev:client": "webpack --config webpack/ssr/webpack.ssr.client.js --mode development --watch",
    "dev:server": "webpack --config webpack/ssr/webpack.ssr.server.js --mode development --watch",
    "dev:both": "yarn clean && npx concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "run:dev": "nodemon --watch dist dist/server.js --trace-warnings",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:styles": "stylelint \"**/*.scss\""
  },
  "engines": {
    "node": ">=22.12.0",
    "yarn": ">=1.22.19"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 iOS versions",
    "last 2 ios_saf versions",
    "last 2 Safari versions"
